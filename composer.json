{
    "name": "areia-lab/slug-uid",
    "description": "A Laravel package to generate unique slugs, UIDs, and sequence numbers.",
    "keywords": [
        "laravel",
        "slug",
        "uid",
        "sequence",
        "generator",
        "unique"
    ],
    "homepage": "https://github.com/areia-lab/slug-uid",
    "license": "MIT",
    "authors": [
        {
            "name": "Akramul Hoque",
            "email": "akramulhoque.engineer@gmail.com"
        }
    ],
    "type": "library",
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "AreiaLab\\SlugUid\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AreiaLab\\SlugUid\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AreiaLab\\SlugUid\\SlugUidServiceProvider"
            ],
            "aliases": {
                "SlugUid": "AreiaLab\\SlugUid\\Facades\\SlugUid"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
